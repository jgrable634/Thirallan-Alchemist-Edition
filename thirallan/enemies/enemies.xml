          <pre><play object="this" animation="charge_up" /></pre>
          <projectile tile="soul_blast" animation="soul_blast" range="20" speed="2" homing="true">
						<onHit>
							<sound id="item" /><set id="corruptedItem" value="corruptItem(player)" global="true" />
